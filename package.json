    "@types/react-dom": "^18.0.9",
    "@types/rimraf": "^3.0.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.0",
    "babel-preset-react-app": "^10.0.1",
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
